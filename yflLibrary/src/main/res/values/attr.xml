<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyBarStyle">
        <attr name="barBg" format="color" />
        <attr name="bgDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="flFlow" format="boolean" />
        <!--        每个item间隔-->
        <attr name="flChildSpacing" format="enum|dimension">
            <enum name="auto" value="-65536" />
        </attr>
        <!--        最小间隔-->
        <attr name="flMinChildSpacing" format="dimension" />
        <!--        最后一排的item间距-->
        <attr name="flChildSpacingForLastRow" format="enum|dimension">
            <enum name="auto" value="-65536" />
            <enum name="align" value="-65537" />
        </attr>
        <!--        上下间距-->
        <attr name="flRowSpacing" format="enum|dimension">
            <enum name="auto" value="-65536" />
        </attr>
        <!--       数据排版 是否从右面开始-->
        <attr name="flRtl" format="boolean" />
        <!--        最大行数-->
        <attr name="flMaxRows" format="integer" />
        <!--        垂直重力-->
        <attr name="flRowVerticalGravity" format="enum">
            <enum name="auto" value="-65536" />
            <enum name="top" value="0x30" />
            <enum name="center" value="0x10" />
            <enum name="bottom" value="0x50" />
        </attr>
        <!--        item 位置-->
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="gridPasswordView">

        <attr name="gpvTextColor" format="color|reference" />
        <attr name="gpvTextSize" format="dimension" />

        <attr name="gpvLineColor" format="color" />
        <attr name="gpvGridColor" format="color" />
        <attr name="gpvLineWidth" format="dimension" />

        <attr name="gpvPasswordLength" format="integer" />
        <attr name="gpvPasswordTransformation" format="string" />

        <attr name="gpvPasswordType" format="enum">
            <enum name="numberPassword" value="0" />
            <enum name="textPassword" value="1" />
            <enum name="textVisiblePassword" value="2" />
            <enum name="textWebPassword" value="3" />
        </attr>

    </declare-styleable>
    <declare-styleable name="CEditText">
        <attr name="height" format="dimension" />
        <attr name="count" format="integer" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="focusLineColor" format="color" />
        <attr name="stokesColor" format="color" />
        <attr name="focusStokeColor" format="color" />
        <attr name="spaceWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="circleRadius" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="isDrawCircle" format="boolean" />
        <attr name="isDrawLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PayPsdInputView">
        <attr name="maxCount" format="integer" />
        <attr name="circleColorOne" format="color" />
        <attr name="bottomLineColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="divideLineWidth" format="dimension" />
        <attr name="divideLineColor" format="color" />
        <attr name="rectAngle" format="dimension" />
        <attr name="focusedColor" format="color" />
        <attr name="psdType" format="enum">
            <enum name="weChat" value="0" />
            <enum name="bottomLine" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RxAutoImageView">
        <attr name="ImageSrc" format="reference" />
    </declare-styleable>
    <declare-styleable name="GmTitleBarView">

        <!--         左侧文字-->
        <attr name="lift_content" format="string" />
        <!--         左侧文字大小-->
        <attr name="lift_size" format="dimension" />
        <!--         左侧文字颜色-->
        <attr name="lift_color" format="color" />
        <!--         左侧背景-->
        <attr name="lift_bg" format="color" />
        <!--         左侧背景-->
        <attr name="lift_resource" format="reference" />
        <!--         中间文字-->
        <attr name="content_content" format="string" />
        <!--         中间文字大小-->
        <attr name="content_size" format="dimension" />
        <!--         中间文字颜色-->
        <attr name="content_color" format="color" />
        <!--         中间文字背景-->
        <attr name="content_bg" format="color" />
        <!--         中间文字资源-->
        <attr name="content_resource" format="reference" />
        <!--         右侧文字-->
        <attr name="right_content" format="string" />
        <!--         右侧文字大小-->
        <attr name="right_size" format="dimension" />
        <!--         右侧文字颜色-->
        <attr name="right_color" format="color" />
        <!--         右侧背景-->
        <attr name="right_bg" format="color" />
        <!--         资源背景-->
        <attr name="right_resource" format="reference" />
        <!--         左图图片-->
        <attr name="ic_life_resource" format="reference" />
        <!--         右图图片-->
        <attr name="ic_rigth_resource" format="reference" />
        <!--         左图显示-->
        <attr name="tv_life_boolean" format="boolean" />
        <!--         右图显示-->
        <attr name="tv_right_boolean" format="boolean" />
        <!--        背景-->
        <attr name="app_bar_bg" format="color" />
        <!--        资源背景-->
        <attr name="app_bar_resource" format="reference" />

        <!--        背景-->
        <attr name="app_title_bar_bg" format="color" />
        <!--        资源背景-->
        <attr name="app_title_bar_resource" format="reference" />
    </declare-styleable>

    <declare-styleable name="CalendarPickerView">
        <attr name="android:background" />
        <attr name="dividerColor" format="color" />
        <attr name="dayBackground" format="reference" />
        <attr name="dayTextColor" format="color" />
        <attr name="dayHeaderTextColor" format="color" />
        <attr name="titleTextStyle" format="reference" />
        <attr name="displayHeader" format="boolean" />
        <attr name="displayDayNamesHeaderRow" format="boolean" />
        <attr name="displayAlwaysDigitNumbers" format="boolean" />
        <attr name="headerTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="calendar_cell">
        <attr name="state_selectable" format="boolean" />
        <attr name="state_current_month" format="boolean" />
        <attr name="state_today" format="boolean" />
        <attr name="state_range_first" format="boolean" />
        <attr name="state_range_middle" format="boolean" />
        <attr name="state_range_last" format="boolean" />
        <attr name="state_highlighted" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AttachButton">
        <!--是否需要自动吸边-->
        <attr name="customIsAttach" format="boolean" />
        <!--是否可拖曳-->
        <attr name="customIsDrag" format="boolean" />
    </declare-styleable>


    <declare-styleable name="SwipeMenuLayout">
        <attr name="leftViewId" format="reference" />
        <attr name="rightViewId" format="reference" />
        <attr name="contentViewId" format="reference" />
    </declare-styleable>

    <declare-styleable name="SearchText">
        <attr name="drawableRes" format="reference" />
        <attr name="clearImgRes" format="reference" />
        <attr name="textSearchColor" format="color" />
        <attr name="hineSearchText" format="string" />

    </declare-styleable>


    <declare-styleable name="TitleCustomBar">
        <!--        自定义背景-->
        <attr name="custombarBg" format="color" />
        <!--        自定义背景图片-->
        <attr name="custombgDrawable" format="reference" />
        <!--         自定义左侧图片-->
        <attr name="customIvLifeRes" format="reference" />
        <!--         自定义右侧图片-->
        <attr name="customIvRightRes" format="reference" />
        <!--        右侧文字-->
        <attr name="customRightText" format="string" />
        <!--        右侧文字颜色-->
        <attr name="customRightColor" format="color" />
        <!--        右侧大小-->
        <attr name="customRightSize" format="dimension" />
        <!--        中间文字-->
        <attr name="customContentText" format="string" />
        <!--        中间颜色-->
        <attr name="customContentColor" format="color" />
        <!--        中间大小-->
        <attr name="customContentSize" format="dimension" />
        <!--        自定义大小格式-->
        <attr name="customtextStyle" format="integer">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <!--        是否显示右侧-->
        <attr name="showTvRight" format="boolean" />

    </declare-styleable>
    <declare-styleable name="CustomMarqueeView">
        <attr name="customScrollSpeed">
            <enum name="fast" value="9" />
            <enum name="medium" value="6" />
            <enum name="slow" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SwipePanel">
        <!--        颜色-->
        <attr name="leftSwipeColor" format="color" />
        <attr name="topSwipeColor" format="color" />
        <attr name="rightSwipeColor" format="color" />
        <attr name="bottomSwipeColor" format="color" />
        <!--        触发阈值-->
        <attr name="leftEdgeSize" format="dimension" />
        <attr name="topEdgeSize" format="dimension" />
        <attr name="rightEdgeSize" format="dimension" />
        <attr name="bottomEdgeSize" format="dimension" />
        <!--        icon-->
        <attr name="leftDrawable" format="reference" />
        <attr name="topDrawable" format="reference" />
        <attr name="rightDrawable" format="reference" />
        <attr name="bottomDrawable" format="reference" />
        <!--        是否居中-->
        <attr name="isLeftCenter" format="boolean" />
        <attr name="isTopCenter" format="boolean" />
        <attr name="isRightCenter" format="boolean" />
        <attr name="isBottomCenter" format="boolean" />
        <!--        是否可用-->
        <attr name="isLeftEnabled" format="boolean" />
        <attr name="isTopEnabled" format="boolean" />
        <attr name="isRightEnabled" format="boolean" />
        <attr name="isBottomEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NiceImageView">
        <!--       是否显示为圆形（默认为矩形）-->
        <attr name="is_circle" format="boolean" />
        <!--        border、inner_border是否覆盖图片内容-->
        <attr name="is_cover_src" format="boolean" />
        <!--        统一设置四个角的圆角半径-->
        <attr name="corner_radius" format="dimension" />
        <!--        左上角圆角半径-->
        <attr name="corner_top_left_radius" format="dimension" />
        <!--        右上角圆角半径-->
        <attr name="corner_top_right_radius" format="dimension" />
        <!--        左下角圆角半径-->
        <attr name="corner_bottom_left_radius" format="dimension" />
        <!--        右下角圆角半径-->
        <attr name="corner_bottom_right_radius" format="dimension" />
        <!--        边框宽度-->
        <attr name="border_width" format="dimension" />
        <!--        边框颜色-->
        <attr name="border_color" format="color" />
        <!--        相当于内层边框（is_circle为true时支持）-->
        <attr name="inner_border_width" format="dimension" />
        <!--        内边框颜色-->
        <attr name="inner_border_color" format="color" />
        <!--        图片上绘制的遮罩颜色-->
        <attr name="mask_color" format="color" />
    </declare-styleable>
</resources>