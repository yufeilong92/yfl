<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleCustomBar">
        <attr name="custombarBg" format="color" />
        <attr name="custombgDrawable" format="reference" />

        <attr name="customIvLifeRes" format="reference" />

        <attr name="customIvRightRes" format="reference" />

        <attr name="customRightText" format="string" />
        <attr name="customRightColor" format="color" />
        <attr name="customRightSize" format="dimension" />

        <attr name="customContentText" format="string" />
        <attr name="customContentColor" format="color" />
        <attr name="customContentSize" format="dimension" />
        <attr name="customtextStyle"  format="integer" >
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="showTvRight" format="boolean" />

    </declare-styleable>
</resources>